<div class="visualn-newsfeed-html-basic-wrapper">
  {% if header_title %}
    <h3>{{ header_title }}</h3>
  {% endif %}
  {% for row in data %}
  <div class="row-wrapper">
    <div class="col-1"><img src="{{ row.image_url }}" width="100" height="100" /></div>
    <div class="col-2">
      <div class="title">
        <a href="{{ row.link }}">{{ row.title }}</a>
      </div>
      <div class="text">
        {{ row.text }}
      </div>
    </div>
  </div>
  {% endfor %}
</div>
